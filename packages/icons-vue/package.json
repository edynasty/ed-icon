{
    "name": "@ed-ui/icons-vue",
    "version": "1.0.0",
    "description": "ED Icons Vue 组件库 - 基于 @ed-ui/icons-svg 的 Vue 3 组件",
    "keywords": [
        "icons",
        "svg",
        "vue",
        "vue3",
        "components"
    ],
    "homepage": "https://github.com/edynasty/ed-icon#readme",
    "bugs": {
        "url": "https://github.com/edynasty/ed-icon/issues"
    },
    "repository": {
        "type": "git",
        "url": "git+https://github.com/edynasty/ed-icon.git",
        "directory": "packages/icons-vue"
    },
    "license": "Apache-2.0",
    "author": {
        "name": "edynasty",
        "email": "1943902972@qq.com"
    },
    "main": "lib/index.js",
    "module": "es/index.js",
    "types": "lib/index.d.ts",
    "files": [
        "lib",
        "es",
        "README.md"
    ],
    "scripts": {
        "build": "npm run generate && npm run clean && npm run build:lib && npm run build:es",
        "build:lib": "tsc -p tsconfig.build.json --outDir lib --module commonjs",
        "build:es": "tsc -p tsconfig.build.json --outDir es --module esnext",
        "clean": "rimraf lib es",
        "generate": "node scripts/generate.js"
    },
    "dependencies": {
        "ejs": "^3.1.10"
    },
    "peerDependencies": {
        "vue": ">=3.0.0"
    },
    "devDependencies": {
        "@types/node": "^18.0.0",
        "rimraf": "^4.0.0",
        "ts-node": "^10.0.0",
        "typescript": "^4.9.0",
        "vue": "^3.3.0"
    },
    "publishConfig": {
        "access": "public",
        "registry": "https://registry.npmjs.org/"
    }
}